{
  "name": "appsensorlike",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen_server_config_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-server-config_schema.json IServerConfiguration",
    "gen_client_config_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-client-config_schema.json ClientConfiguration",
    "gen_mysql_mapping_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o mapping_schema.json IMapping",
    "gen_storage_mysql_config_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-storage-mysql-config_schema.json MySQLStorageConfig",
    "gen_reporting_websocket_server_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-websocket-server-config_schema.json IWebSocketServerConfig",
    "gen_reporting_websocket_client_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-websocket-client-config_schema.json WebSocketClientConfig",
    "run_reporting_tests_server_only": "npx run-func ./reporting-engines/appsensor-reporting-websocket/tests/appsensor-reporting-websocket-tests.js runServerSeparately",
    "run_reporting_tests_client_only": "npx run-func ./reporting-engines/appsensor-reporting-websocket/tests/appsensor-reporting-websocket-tests.js runClientSeparately",
    "run_reporting_tests_client_only_report_events": "npx run-func ./reporting-engines/appsensor-reporting-websocket/tests/appsensor-reporting-websocket-tests.js runClientSeparatelyReportEvents",
    "gen_logging_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-logging-config_schema.json LoggingConfiguration",
    "gen_rest_server_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-rest-server-config_schema.json RestServerConfig",
    "gen_rest_client_schema": "typescript-json-schema tsconfig.json --noExtraProps true --required true -o appsensor-rest-client-config_schema.json RestClientConfig"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/si076/-appsensorlike.git"
  },
  "keywords": [
    "js",
    "appsensor",
    "json"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/si076/-appsensorlike/issues"
  },
  "homepage": "https://github.com/si076/-appsensorlike#readme",
  "dependencies": {
    "@inquirer/prompts": "^1.2.3",
    "@js-joda/core": "^5.5.3",
    "@js-joda/extra": "^0.11.3",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.0-rc.0",
    "cli-spinner": "^0.2.10",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.6",
    "express": "^4.18.2",
    "fast-geoip": "^1.1.88",
    "hbs": "^4.2.0",
    "inquirer-date-prompt": "^3.0.0",
    "inquirer-interrupted-prompt": "^3.0.0",
    "ipaddr.js": "^2.0.1",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "table": "^6.8.1",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/cli-spinner": "^0.2.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hbs": "^4.0.1",
    "@types/log4js": "^2.3.5",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.15.1",
    "@types/table": "^6.3.2",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4"
  }
}
